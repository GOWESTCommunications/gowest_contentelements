                </TCEforms>
                <type>array</type>
                <el>
                    <settings.items>
                        <section>1</section>
                        <type>array</type>
                        <title>LLL:EXT:gowest_contentelements/Resources/Private/Language/locallang_flexform.xml:anchornavigation.title</title>
                        <el>
                        <configuration>
